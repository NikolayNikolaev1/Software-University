<h1>Projects</h1>
<h3>Manage Teams</h3>
<div class="form">
    <form action="/admin/projects" method="POST">
        <label>Team</label>
        <select name="teams">
            {{#each teams}}
            <option value="{{_id}}">{{name}}</option>
            {{/each}}
        </select>
        <label>Project</label>
        <select name="projects">
            {{#each projects}}
            <option value="{{_id}}">{{name}}</option>
            {{/each}}
        </select>
        <input type="submit" value="Distribute">
    </form>
</div>